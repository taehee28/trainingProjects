<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.thk.layoutsample2.HomeFragment"
        android:label="HomeFragment" >
        <action
            android:id="@+id/action_homeFragment_to_constraintMainFragment"
            app:destination="@id/constraintMainFragment" />
    </fragment>
    <fragment
        android:id="@+id/constraintMainFragment"
        android:name="com.thk.layoutsample2.ConstraintMainFragment"
        android:label="ConstraintMainFragment" >
        <action
            android:id="@+id/action_constraintMainFragment_to_constraintBasicFragment"
            app:destination="@id/constraintBasicFragment" />
        <action
            android:id="@+id/action_constraintMainFragment_to_constraintDimensionFragment"
            app:destination="@id/constraintDimensionFragment" />
        <action
            android:id="@+id/action_constraintMainFragment_to_constraintChainFragment"
            app:destination="@id/constraintChainFragment" />
        <action
            android:id="@+id/action_constraintMainFragment_to_constraintFlowFragment"
            app:destination="@id/constraintFlowFragment" />
        <action
            android:id="@+id/action_constraintMainFragment_to_constraintGuidelineFragment"
            app:destination="@id/constraintGuidelineFragment" />
        <action
            android:id="@+id/action_constraintMainFragment_to_constraintBarrierFragment"
            app:destination="@id/constraintBarrierFragment" />
    </fragment>
    <fragment
        android:id="@+id/constraintBasicFragment"
        android:name="com.thk.layoutsample2.ConstraintBasicFragment"
        android:label="ConstraintBasicFragment" />
    <fragment
        android:id="@+id/constraintDimensionFragment"
        android:name="com.thk.layoutsample2.ConstraintDimensionFragment"
        android:label="ConstraintDimensionFragment" />
    <fragment
        android:id="@+id/constraintChainFragment"
        android:name="com.thk.layoutsample2.ConstraintChainFragment"
        android:label="ConstraintChainFragment" />
    <fragment
        android:id="@+id/constraintFlowFragment"
        android:name="com.thk.layoutsample2.ConstraintFlowFragment"
        android:label="ConstraintFlowFragment" />
    <fragment
        android:id="@+id/constraintGuidelineFragment"
        android:name="com.thk.layoutsample2.ConstraintGuidelineFragment"
        android:label="ConstraintGuidelineFragment" />
    <fragment
        android:id="@+id/constraintBarrierFragment"
        android:name="com.thk.layoutsample2.ConstraintBarrierFragment"
        android:label="ConstraintBarrierFragment" />
</navigation>
<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.thk.layoutsample2.HomeFragment"
        android:label="HomeFragment" >
        <action
            android:id="@+id/action_homeFragment_to_constraintMainFragment"
            app:destination="@id/constraintMainFragment" />
    </fragment>
    <fragment
        android:id="@+id/constraintMainFragment"
        android:name="com.thk.layoutsample2.ConstraintMainFragment"
        android:label="ConstraintMainFragment" >
        <action
            android:id="@+id/action_constraintMainFragment_to_constraintBasicFragment"
            app:destination="@id/constraintBasicFragment" />
    </fragment>
    <fragment
        android:id="@+id/constraintBasicFragment"
        android:name="com.thk.layoutsample2.ConstraintBasicFragment"
        android:label="ConstraintBasicFragment" />
</navigation>